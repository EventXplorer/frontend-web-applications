{
    "hosting": {
      "public": "dist/eventxplorer",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        },
        {
          "source": "/api/**",
          "destination": "http://localhost:3000/users"
        }
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  }
  
